{
  "name": "lilianalillyy/nubium-trial",
  "description": "Nubium trial task",
  "type": "project",
  "require": {
    "php": ">= 8.1",
    "nette/application": "^3.1",
    "contributte/bootstrap": "^0.5",
    "nette/caching": "^3.1",
    "nette/database": "^3.1",
    "nette/di": "^3.0",
    "nette/finder": "^2.5",
    "nette/forms": "3.1.7",
    "nette/http": "^3.1",
    "nette/mail": "^3.1",
    "nette/robot-loader": "^3.3",
    "nette/security": "^3.1",
    "nette/utils": "^3.2",
    "latte/latte": "^2.9",
    "tracy/tracy": "^2.8",
    "contributte/forms-bootstrap": "^0.5.3",
    "contributte/neonizer": "^0.5.0",
    "phpstan/phpstan": "^1.9",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.1",
    "phpstan/phpstan-strict-rules": "^1.4",
    "nettrine/orm": "^0.8.3",
    "nettrine/dbal": "^0.8.0",
    "nettrine/cache": "^0.3.0",
    "contributte/console": "^0.9.2",
    "nettrine/fixtures": "^0.6.3",
    "nettrine/migrations": "^0.8.0",
    "ramsey/uuid-doctrine": "^1.8",
    "gedmo/doctrine-extensions": "^3.10",
    "contributte/webpack": "^2.2"
  },
  "scripts": {
    "post-install-cmd": [
      "Contributte\\Neonizer\\NeonizerExtension::process"
    ],
    "post-update-cmd": [
      "Contributte\\Neonizer\\NeonizerExtension::process"
    ]
  },
  "autoload": {
    "psr-4": {
      "App\\": "app",
      "Database\\": "db"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "neonizer": {
      "files": [
        {
          "dist-file": "config/local.neon.dist"
        }
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/thanks": false
    }
  },
  "require-dev": {
    "fakerphp/faker": "^1.20"
  }
}
